{
  "name": "queuetube",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "host": "svelte-kit dev --host",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "prepare": "svelte-kit sync && npm run smui-theme-base && npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-base": "smui-theme compile static/smui-base.css -i src/theme",
    "smui-theme-light": "smui-theme compile static/smui-light.css -i src/theme/light",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@material/typography": "^13.0.0",
    "@playwright/test": "^1.21.0",
    "@smui/button": "*",
    "@smui/textfield": "*",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "*",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "smui-theme": "*",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module"
}

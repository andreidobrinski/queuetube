{
  "name": "queuetube",
  "version": "0.0.1",
  "homepage": "https://andreidobrinski.com/queuetube",
  "scripts": {
    "dev": "vite dev",
    "build": "rm -rf build && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "host": "svelte-kit dev --host",
    "deploy": "gh-pages -d build -t true",
    "package": "svelte-kit package",
    "test": "playwright test",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "prepare": "svelte-kit sync && npm run smui-theme-base && npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-base": "smui-theme compile static/smui-base.css -i src/theme",
    "smui-theme-light": "smui-theme compile static/smui-light.css -i src/theme/light",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
    "restart": "npm run prepare && npm run dev"
  },
  "devDependencies": {
    "@material/typography": "^13.0.0",
    "@playwright/test": "^1.21.0",
    "@smui/button": "6.0.0-beta.16",
    "@smui/circular-progress": "*",
    "@smui/common": "6.0.0-beta.16",
    "@smui/dialog": "6.0.0-beta.16",
    "@smui/icon-button": "6.0.0-beta.16",
    "@smui/list": "6.0.0-beta.16",
    "@smui/menu": "6.0.0-beta.16",
    "@smui/snackbar": "*",
    "@smui/tab": "6.0.0-beta.16",
    "@smui/tab-bar": "6.0.0-beta.16",
    "@smui/textfield": "6.0.0-beta.16",
    "@sveltejs/adapter-static": "2.0.3",
    "@sveltejs/kit": "^1.20.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "gh-pages": "^4.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.10.1",
    "smui-theme": "6.0.0-beta.16",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.3.1",
    "typescript": "^5.0.0",
    "vite": "^4.0.0"
  },
  "type": "module",
  "dependencies": {
    "axios": "^0.27.2",
    "dayjs": "^1.11.2"
  }
}